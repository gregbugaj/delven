{
  "name": "@delven/blueprint",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "private": true,
  "author": "Greg",
  "dependencies": {
    "concurrently": "^7.0.0",
    "eslint": "^8.12.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "watch:server": "NODE_ENV=development nodemon build/index.js --watch build",
		"watch:compile": "tsc -b ./tsconfig.json -w",
    "watch": "concurrently --kill-others \"yarn run watch:compile\" \"yarn run watch:server\" ",
    "build": "NODE_ENV=development tsc -b ./tsconfig.json",
    "lint": "eslint 'src/**/*.ts' || true",
    "prettier": "prettier --write 'src/**/*.ts'",
    "check-types": "tsc"
  },
  "devDependencies": {

    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "body-parser": "^1.19.0",
    "bufferutil": "^4.0.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "inversify": "^5.1.1",
    "nodemon": "^2.0.15",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "prettier": "^2.6.1",
    "reflect-metadata": "^0.1.13",
    "source-map-loader": "^3.0.1",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "typescript": "^4.7.0-dev.20220331",
    "utf-8-validate": "^5.0.9",
    "yarn": "^1.22.17"
  }
}
