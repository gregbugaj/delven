{
    "name": "delven-transpiler",
    "version": "1.0.0",
    "description": "delven transpiler",
    "main-ts": "src/index.ts",
    "main": "dist/index.js",
    "scripts": {
        "g4": "java -Xmx500M -cp \"/usr/local/lib/antlr/antlr-4.8-complete.jar:$CLASSPATH\" org.antlr.v4.Tool -Dlanguage=JavaScript ./src/grammar/*.g4 -visitor -o ./src/parser",
        "test": "jest",
        "build": "./node_modules/.bin/babel --watch src --out-dir dist --extensions '.ts,.js'",
        "check-types": "tsc",
        "watch-ts": "tsc -w",
        "build-ts": "./node_modules/.bin/babel src --out-dir dist --extensions '.ts,.js'",
        "lint-fix": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
        "lint": " eslint \"**/*.{js,ts}\" --quiet ",
        "prettier": "prettier \"*/**/*.ts*\" --ignore-path ./.prettierignore --write",
        "buildXXXX": "npm run build-ts && npm run lint ",
        "package": "webpack --config babel.webpack.config.js",
        "types": "tsc -p tsconfig.build.types.json --outDir ./types"
    },
    "author": "Greg",
    "license": "ISC",
    "devDependencies": {
        "@babel/cli": "^7.10.1",
        "@babel/core": "^7.12.10",
        "@babel/node": "^7.10.1",
        "@babel/plugin-proposal-class-properties": "^7.10.1",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
        "@babel/plugin-proposal-optional-chaining": "^7.10.1",
        "@babel/plugin-transform-runtime": "^7.10.1",
        "@babel/preset-env": "^7.11.5",
        "@babel/preset-typescript": "^7.10.1",
        "@types/antlr4": "^4.7.1",
        "@types/jest": "^25.2.3",
        "@types/node": "^14.0.12",
        "@types/node-fetch": "^2.5.7",
        "@typescript-eslint/eslint-plugin": "^3.3.0",
        "@typescript-eslint/parser": "^3.3.0",
        "antlr4": "^4.8.0",
        "babel-loader": "^8.2.2",
        "copy-webpack-plugin": "^6.1.0",
        "eslint": "^7.2.0",
        "eslint-config-standard": "^14.1.1",
        "eslint-plugin-import": "^2.21.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.0.1",
        "jest": "^26.1.0",
        "nodemon": "^2.0.4",
        "prettier": "^1.9.2",
        "ts-jest": "^26.1.1",
        "typescript": "^3.9.5",
        "webpack": "^4.44.1",
        "webpack-cli": "^3.3.11"
    },
    "dependencies": {
        "@babel/runtime": "^7.10.2",
        "chalk": "^4.1.0",
        "glob": "^7.1.6",
        "json-diff": "^0.5.4",
        "jsondiffpatch": "^0.4.1",
        "node-fetch": "^2.6.1"
    }
}
